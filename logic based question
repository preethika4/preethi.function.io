//output 20 60 104 152 260
#include <stdio.h>

int logic(int n){
    int s1=20,s2=40;
    int d;
    if(n==1){
        printf("%d ",s1);
    }
    
    else{
        printf("%d ",s1);
        s1+=s2;
        printf("%d ",s1);
        for(int i=3;i<=n;i++){
            d=4*(i-2);
            s1+=s2+d;
            printf("%d ",s1);
        }
    }

}
int main()
{
    int n;
    scanf("%d",&n);
     logic(n);
    



    return 0;
}
